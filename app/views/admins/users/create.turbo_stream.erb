<% if @user.errors.any? %>
  <%= turbo_stream.replace "#{dom_id(User.new)}_form" do %>
    <%= render "admins/users/partials/form", user: @user %>
  <% end %>
<% else %>
  <%= (turbo_stream.prepend "users-tbody", partial: "admins/users/partials/user", 
      locals: { user: @user }) if @user.type == 'User' && params[:page] == '1' %>
<% end %>
<%= render_turbo_stream_flash_messages %>
