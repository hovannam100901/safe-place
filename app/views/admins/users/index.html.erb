<div class="card w-80 mb-3 mt-3">
  <div class="card-body">
    <h5 class="card-title">Search User</h5>
    <%= render 'admins/partials/search_user_form', q: @q, path: admins_users_path %>
  </div>
</div>
<div class="profile-card">
  <div class="card w-80">
    <div class="card-body">
      <div class="card-title card-title-user d-flex justify-content-between align-items-center ">
        <h1>List Users</h1>
        <button class="button_hidden" data-bs-toggle="modal" data-bs-target="#exampleModal">
          <%= link_to "+ New User",new_admins_user_path(page: params[:page]), data:{turbo_frame: "modal_for_user"},class: "btn btn-lg text-white button_edit_user"%>
        </button>
      </div>
      <ul class="nav nav-tabs nav-fill ">
        <li class="nav-item">
          <%= link_to 'Users', admins_users_path, data:{ turbo_frame: "table-user" }, class: "nav-link active" %>
        </li>
        <li class="nav-item">
          <%= link_to 'Counselors', admins_counselors_path, data:{ turbo_frame: "table_user" }, class: "nav-link" %>
        </li>
      </ul>
      <p class="card-text">
        <%= turbo_frame_tag "table-user" do %>
          <%= render "admins/users/partials/user_table", users: @users %>
        <% end %>
      </p>
    </div>
  </div>
</div>
<%= render 'admins/users/partials/modal' %>
